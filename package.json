{
  "repository": {
    "url": "https://github.com/davedawkins/Fable.Sveltish.git"
  },
  "private": true,
  "scripts": {
    "postinstall": "dotnet tool restore",
    "start": "dotnet fable watch src/App --run webpack serve",
    "build": "dotnet fable src/App --run webpack --mode production",
    "build:tests": "dotnet fable tests --run webpack --mode production",
    "publish": "node publish.js",
    "test": "mocha dist/tests"
  },
  "dependencies": {
    "gh-pages": "^3.1.0",
    "mocha": "^8.2.1",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
